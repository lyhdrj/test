<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<include file="struts-sysmanage.xml"/> 
	<include file="struts-baseinfo.xml"/> 
	<constant name="struts.ui.theme" value="simple" />
	<constant name="struts.custom.i18n.resources" value="com.nasoft.ApplicationResources"></constant>
	<constant name="struts.i18n.encoding" value="gbk"></constant>
	<package name="default" extends="struts-default">
		<default-interceptor-ref name="paramsPrepareParamsStack" />		
		<global-results>
			<result name="error">/page/error/error.jsp</result>
			<result name="serviceExceptionMessage">/page/common/showServiceExceptionMessage.jsp</result>
    	</global-results>
	</package>
	
</struts>